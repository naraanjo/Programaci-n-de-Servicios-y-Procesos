package factorial;

import java.io.BufferedReader;
import java.io.BufferedWriter;
import java.io.File;
import java.io.InputStreamReader;
import java.util.ArrayList;

public class P2 {

	public static void main(String[] args) throws Exception {
		
		// Leo la info de PP
		// La escribo en un fichero
		String linea = "";
		ArrayList<String> listaInfo = new ArrayList<String>();
		
		try(BufferedReader br = new BufferedReader(new InputStreamReader(System.in))){
			while((linea = br.readLine()) != null) {
					listaInfo.add(linea);
			}
		}
		
		
		// Escribo la info en un fichero
		File archivo = new File("resultadoFactoriales");
		if(!archivo.exists()) archivo.createNewFile();
		
		
		// Envio la info al PP
		System.out.println("Resultados enviados al fichero cone exito");
	}
}
